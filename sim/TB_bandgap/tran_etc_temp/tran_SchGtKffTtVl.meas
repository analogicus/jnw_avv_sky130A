* Measure TB_bandgap
.control

load tran_SchGtKffTtVl.raw

echo "MEAS_START"

set fend = .raw

echo tran_SchGtKffTtVl

foreach vtemp -40 -20 0 20 40 60 80 100 120
load tran_SchGtKffTtVl_$vtemp$fend
meas tran Vref_$vtemp avg v(vref) from=50u to=69u
end

echo "MEAS_END"
echo tran_SchGtKffTtVl

.endc


