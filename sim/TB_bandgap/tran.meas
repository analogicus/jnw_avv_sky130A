* Measure TB_bandgap
.control

load {cicname}.raw

echo "MEAS_START"

set fend = .raw

* foreach vtemp 20
foreach vtemp -40 -20 0 20 40 60 80 100 125
load {cicname}_$vtemp$fend
meas tran Vn_$vtemp avg v(xdut.vn) from=110u to=120u
meas tran Vp_$vtemp avg v(xdut.vp) from=110u to=120u
meas tran Vref_$vtemp avg v(vref) from=110u to=120u
meas tran ivdd_$vtemp avg i(vdd) from=110u to=120u
end

echo "MEAS_END"
echo {cicname}

.endc


